            "required": false,
            "schema": {
              "type": "string",
              "description": "Country code (e.g., 'us' for the United States, 'fr' for France).",
              "default": "us",
              "title": "Country"
            },
            "description": "Country code (e.g., 'us' for the United States, 'fr' for France)."
          },
          {
            "name": "pages",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 10,
              "minimum": 1,
              "description": "Number of pages to scrape (1-10).",
