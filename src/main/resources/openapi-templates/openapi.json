            },
            "description": "Number of pages to scrape (1-10)."
          },
          {
            "name": "source",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "description": "Job source to scrape (e.g., 'indeed', 'linkedin').",
              "default": "indeed",
              "title": "Source"
            },
            "description": "Job source to scrape (e.g., 'indeed', 'linkedin')."
          }
        ],
